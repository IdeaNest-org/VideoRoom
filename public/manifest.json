{
    "name": "Video Room",
    "description": "Watch video with your Friends",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": ["storage", "http://*/*", "https://*/*"],
    "content_scripts": [
        {
            "matches": ["https://www.mgtv.com/*"],
            "js": ["controller.js"],
            "css": [],
            "run_at": "document_end"
        }
    ],
    "homepage_url": "https://github.com/IdeaNest-org/videoRoom",
    "icons": {
        "16": "logo.png",
        "32": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
    }
}
