{
    "name": "Video Room",
    "description": "Watch video with your Friends",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": ["storage", "http://*/*", "https://*/*"],
    "browser_action": {
        "default_icon": "logo.png",
        "default_title": "Video Room",
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["controller.js"],
            "css": [],
            "run_at": "document_end"
        }
    ],
    "homepage_url": "https://github.com/IdeaNest-org/videoRoom",
    "icons": {
        "16": "logo.png",
        "32": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://demos.focusbe.com http://localhost:3000; object-src 'self'"
}
